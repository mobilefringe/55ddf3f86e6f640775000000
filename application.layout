<script>
    var current_year = moment().year();
    $("#current_year").text(current_year);
    
    $("#mobile_menu_btn").click(function() {
       toggle_mobile_menu(); 
    });
    
    $("#close_form").click(function() {
        close_form();
    });
    
    $("#close_popup").click(function() {
        close_form();    
    });
    
    $("#popup_form").submit(function(e) {
        e.preventDefault();
        if ($("#popup_agree_terms").is(":checked")) {
            if (check_email("fieldEmail") == true) {
                $("#fieldtydytu").val($("#agree_terms").attr("value"));
                $.getJSON("//mobilefringe.createsend.com/t/d/s/dljtlh/?callback=?", $(this).serialize(), function (data) {
                    if (data.Status === 400) {
                        e.preventDefault();
                        alert("Please try again later.");
                    } else {
                        $(".form_div").hide();
                        $(".thank_you_div").fadeIn();
                    }
                }); 
            } else {
                alert("Please enter a valid email address")
            }
        } else {
            alert("Please aggree to receive emails from Heartland Town Centre.")
        }
    });

    $("#newsletter_form").submit(function(e) {
        e.preventDefault();
        if (check_email("desktop_email") == true) {
            $("#fieldEmail").val($("#desktop_email").val());
            $(".form_div").show();
            $(".thank_you_div").hide();
            $(".newsletter_overlay").fadeIn();
            $(".newsletter_container").fadeIn();  
        } else {
            alert("Please enter a valid email address")
        }
    });
    
    $("#mobile_newsletter_form").submit(function(e) {
        e.preventDefault();
        if (check_email("mobile_email") == true) {
            $("#fieldEmail").val($("#mobile_email").val());
            $(".form_div").show();
            $(".thank_you_div").hide();
            $(".newsletter_overlay").fadeIn();
            $(".newsletter_container").fadeIn();  
        } else {
            alert("Please enter a valid email address")
        }
    });

    $('#search_input').keyup(function(e) {
        showSearchResults();
    });

    $('#search_input').on('input', function() {
        showSearchResults();
    });
    
    $(document).ready(function() {
        function renderAll() {
            var propertyDetails = getPropertyDetails();
            renderPageData("#logo_container", "#header_template", propertyDetails, "property");
            
            var featureList = getFeatureList();
            renderPageData("#right_block_container", "#right_block_template", featureList, "right_block")
        }
        loadMallDataCached(renderAll);  
    });
   
    function appLink(){
        var isAndroid = /android/i.test(navigator.userAgent.toLowerCase());
        var isiPod = /ipod/i.test(navigator.userAgent.toLowerCase());
        var isiPhone = /iphone/i.test(navigator.userAgent.toLowerCase());
        var isiPad = /ipad/i.test(navigator.userAgent.toLowerCase());

        if (isAndroid) {
            window.location.href = "https://play.google.com/store/apps/details?id=com.mobilefringe.heartland";
        } else if(isiPod || isiPhone || isiPad){
            window.location.href = "https://itunes.apple.com/ca/app/heartland-town-centre/id1036907331?mt=8";
        }
    }
</script>